<form>
  <label>ACI Classification</label>
  <description>Classification data from dg.log/dglog.bak</description>
  <fieldset submitButton="false">
    <input type="dropdown" token="hosttok" searchWhenChanged="true">
      <label>Host Name</label>
      <fieldForLabel>host</fieldForLabel>
      <fieldForValue>host</fieldForValue>
      <search>
        <query>index=logiq|stats count by host</query>
        <earliest>0</earliest>
        <latest></latest>
      </search>
      <choice value="*">All</choice>
      <default>*</default>
    </input>
    <input type="dropdown" token="acttok" searchWhenChanged="true">
      <label>Action</label>
      <fieldForLabel>eventAction</fieldForLabel>
      <fieldForValue>eventAction</fieldForValue>
      <search>
        <query>index=logiq sourcetype=dglog EventType="ACI_ClassifyFile2"|stats count BY eventAction</query>
        <earliest>0</earliest>
        <latest></latest>
      </search>
      <choice value="*">All</choice>
      <initialValue>*</initialValue>
      <default>*</default>
    </input>
    <input type="dropdown" token="targettok">
      <label>Action Target</label>
      <choice value="*">Any</choice>
      <default>*</default>
      <fieldForLabel>actionTarget</fieldForLabel>
      <fieldForValue>actionTarget</fieldForValue>
      <search>
        <query>index=logiq sourcetype=dglog EventType="ACI_ClassifyFile2"|stats count by actionTarget</query>
        <earliest>0</earliest>
        <latest></latest>
      </search>
    </input>
    <input type="text" token="pathtok" searchWhenChanged="true">
      <label>Path</label>
      <default>*</default>
      <initialValue>*</initialValue>
    </input>
    <input type="text" token="eltok" searchWhenChanged="true">
      <label>Elapsed &amp; Duration &gt; (MS)</label>
      <default>*</default>
      <initialValue>*</initialValue>
    </input>
    <input type="checkbox" token="timetok.earliest">
      <label>Limit to last session for:</label>
      <fieldForLabel>host</fieldForLabel>
      <fieldForValue>sessionStartTime</fieldForValue>
      <search>
        <query>|savedsearch sessionStart hosttok=$hosttok|s$</query>
        <earliest>0</earliest>
        <latest></latest>
      </search>
      <delimiter> </delimiter>
    </input>
    <input type="time" token="timetok" searchWhenChanged="true">
      <label>Time Range</label>
      <default>
        <earliest>0</earliest>
        <latest></latest>
      </default>
    </input>
    <input type="dropdown" token="enttok" searchWhenChanged="true">
      <label>Entity</label>
      <fieldForLabel>entity</fieldForLabel>
      <fieldForValue>entity</fieldForValue>
      <search>
        <query>index=logiq sourcetype=dglog EventType="_DGCI_DoContentInspection"|stats count by entity</query>
        <earliest>0</earliest>
        <latest></latest>
      </search>
      <choice value="*">All</choice>
      <default>*</default>
      <initialValue>*</initialValue>
    </input>
  </fieldset>
  <row>
    <panel>
      <table>
        <title>Total elapsed time per action</title>
        <search>
          <query>index=logiq sourcetype=dglog host=* EventType="ACI_ClassifyFile2" elapsedMS&gt;$eltok$ eventAction=$acttok$ eventSrcPath=$pathtok|s$ OR eventDstPath=$pathtok|s$|stats earliest(_time) as startTime latest(_time) as endTime range(_time) as duration count(eventAction) AS eventCount sum(elapsedMS) AS sumElapsedMS BY eventAction|eval Path=$pathtok|s$|rename Path AS "Path spec"|rename eventAction AS "Action"|sort 0 -sumElapsedMS|eval startTime=strftime(startTime, "%Y-%m-%d %H:%M:%S.%3N")|eval endTime=strftime(endTime, "%Y-%m-%d %H:%M:%S.%3N")|eval duration=tostring(duration, "duration")|eval sumElapsedMS=tostring(sumElapsedMS/1000, "duration")|rename eventCount AS "Action count" sumElapsedMS AS "Total time spent in ACI" startTime AS "Start Time" endTime AS "End Time" duration AS "Event duration"</query>
          <earliest>$timetok.earliest$</earliest>
          <latest>$timetok.latest$</latest>
        </search>
        <option name="drilldown">none</option>
        <option name="refresh.display">progressbar</option>
        <option name="totalsRow">true</option>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <table>
        <title>Elapsed time per action and path</title>
        <search>
          <query>index=logiq sourcetype=dglog host=* EventType="ACI_ClassifyFile2" elapsedMS&gt;$eltok$ eventAction=$acttok$ eventSrcPath=$pathtok|s$ OR eventDstPath=$pathtok|s$|eval actionPath=if(actionTarget="dest",eventDstPath,eventSrcPath)|stats sum(elapsedMS) AS SumElapsedMS by eventAction,actionPath|rename eventAction AS "Action" actionPath AS Path|sort 0 -SumElapsedMS|rename SumElapsedMS AS "Sum of elapsed ms"</query>
          <earliest>$timetok.earliest$</earliest>
          <latest>$timetok.latest$</latest>
        </search>
        <option name="drilldown">row</option>
        <option name="refresh.display">progressbar</option>
        <option name="rowNumbers">true</option>
        <option name="totalsRow">true</option>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <title>time elapsed ms</title>
      <table>
        <search>
          <query>index=logiq sourcetype=dglog host=$hosttok$ EventType="ACI_ClassifyFile2" elapsedMS&gt;$eltok$ eventAction=$acttok$ actionTarget=$targettok$ eventSrcPath=$pathtok|s$ OR eventDstPath=$pathtok|s$|table _time, elapsedMS, eventAction, actionTarget, eventSrcPath, eventDstPath, excluded, not_excluded</query>
          <earliest>$timetok.earliest$</earliest>
          <latest>$timetok.latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="count">25</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">cell</option>
        <option name="percentagesRow">false</option>
        <option name="refresh.display">progressbar</option>
        <option name="rowNumbers">true</option>
        <option name="totalsRow">true</option>
        <option name="wrap">true</option>
        <format type="color" field="elapsed">
          <colorPalette type="list">[#53A051,#F8BE34,#F1813F,#DC4E41]</colorPalette>
          <scale type="threshold">3,6,10</scale>
        </format>
        <drilldown>
          <link target="_blank">/app/logiq/dg_log?$click.name2$=$click.value2$</link>
        </drilldown>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <table>
        <title>Content Inspection Entity Frequency</title>
        <search>
          <query>index=logiq sourcetype=dglog host=$hosttok$ EventType="_DGCI_DoContentInspection" entity=$enttok$|table _time, entity, frequency</query>
          <earliest>$timetok.earliest$</earliest>
          <latest>$timetok.latest$</latest>
        </search>
        <option name="drilldown">none</option>
        <option name="refresh.display">progressbar</option>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <table>
        <title>CI Duration</title>
        <search>
          <query>index=logiq sourcetype=dglog host=$hosttok$ duration&gt;*|table _time, duration, EventType, start_end, File</query>
          <earliest>$timetok.earliest$</earliest>
          <latest>$timetok.latest$</latest>
        </search>
        <option name="drilldown">none</option>
        <option name="refresh.display">progressbar</option>
        <format type="color" field="duration">
          <colorPalette type="list">[#53A051,#006D9C,#F8BE34,#F1813F,#DC4E41]</colorPalette>
          <scale type="threshold">150,300,600,1200</scale>
        </format>
      </table>
    </panel>
  </row>
</form>
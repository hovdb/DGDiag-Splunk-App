<form>
  <label>ProServ-ACI</label>
  <fieldset submitButton="false">
    <input type="dropdown" token="hosttok" searchWhenChanged="true">
      <label>Host Name</label>
      <fieldForLabel>host</fieldForLabel>
      <fieldForValue>host</fieldForValue>
      <search>
        <query>index=logiq|stats count by host</query>
        <earliest>0</earliest>
        <latest></latest>
      </search>
      <choice value="*">All</choice>
      <default>*</default>
    </input>
    <input type="checkbox" token="timetok.earliest">
      <label>Limit to last session for:</label>
      <fieldForLabel>host</fieldForLabel>
      <fieldForValue>sessionStartTime</fieldForValue>
      <search>
        <query>|savedsearch sessionStart hosttok=$hosttok|s$</query>
        <earliest>0</earliest>
        <latest></latest>
      </search>
      <delimiter> </delimiter>
    </input>
    <input type="time" token="timetok" searchWhenChanged="true">
      <label>Time Range</label>
      <default>
        <earliest>0</earliest>
        <latest></latest>
      </default>
    </input>
    <input type="text" token="searchtok" searchWhenChanged="true">
      <label>Search for keyword or string("")</label>
      <default>*</default>
      <initialValue>*</initialValue>
    </input>
  </fieldset>
  <row>
    <panel>
      <event>
        <title>ACI Timeouts due to large files\content</title>
        <search>
          <query>index=logiq sourcetype=dglog host=$hosttok$ EventType="CTextFilter::ReadText" "Keyview Timeout working on file" $searchtok$</query>
          <earliest>$timetok.earliest$</earliest>
          <latest>$timetok.latest$</latest>
        </search>
        <option name="list.drilldown">none</option>
        <option name="refresh.display">progressbar</option>
      </event>
    </panel>
  </row>
  <row>
    <panel>
      <event>
        <title>Retrieve the file and ACI Frequency:</title>
        <search>
          <query>index=logiq sourcetype=dglog host=$hosttok$ (EventType="_DGCI_DoContentInspection" "END CI time for File") OR (EventType="_DGCI_DoContentInspection" "Entity") OR (EventType="CTextExtractor::KV_ExtractText" "duration=") OR (EventType="CTextExtractor::KV_ExtractText" "\*") $searchtok$</query>
          <earliest>$timetok.earliest$</earliest>
          <latest>$timetok.latest$</latest>
        </search>
        <option name="count">100</option>
        <option name="list.drilldown">none</option>
        <option name="refresh.display">progressbar</option>
      </event>
    </panel>
  </row>
  <row>
    <panel>
      <event>
        <title>ACI Inspection Time</title>
        <search>
          <query>index=logiq sourcetype=dglog host=$hosttok$ (EventType="_DGCLASSIFY_DoWork3::timer::DoContentInspection2" "Duration =") OR (EventType="_DGCI_DoContentInspection" "START CI time for File") OR (EventType="_DGCI_DoContentInspection" "END CI time for File") OR (EventType="CTextExtractor::KV_ExtractText" "Total time spent on") $searchtok$</query>
          <earliest>$timetok.earliest$</earliest>
          <latest>$timetok.latest$</latest>
        </search>
        <option name="list.drilldown">none</option>
        <option name="refresh.display">progressbar</option>
      </event>
    </panel>
  </row>
  <row>
    <panel>
      <event>
        <title>ACI_ClassifyFile2 action</title>
        <search>
          <query>index=logiq sourcetype=dglog host=$hosttok$ EventType=ACI_ClassifyFile2 eventAction=* $searchtok$</query>
          <earliest>$timetok.earliest$</earliest>
          <latest>$timetok.latest$</latest>
        </search>
        <option name="list.drilldown">none</option>
        <option name="refresh.display">progressbar</option>
      </event>
    </panel>
  </row>
  <row>
    <panel>
      <event>
        <title>ACI Matches</title>
        <search>
          <query>index=logiq sourcetype=dglog host=$hosttok$ (EventType="CBaseExtractor::retrieveItems" "Match") OR (EventType="CEntityExtractor::retrieveItems" "Match") $searchtok$</query>
          <earliest>$timetok.earliest$</earliest>
          <latest>$timetok.latest$</latest>
        </search>
        <option name="list.drilldown">none</option>
        <option name="refresh.display">progressbar</option>
      </event>
    </panel>
  </row>
  <row>
    <panel>
      <event>
        <title>ACI Threshold Exceeded</title>
        <search>
          <query>index=logiq sourcetype=dglog host=$hosttok$ EventType="CCIProcessor::_ClassifyFile" "Turning content inspection OFF" $searchtok$</query>
          <earliest>$timetok.earliest$</earliest>
          <latest>$timetok.latest$</latest>
        </search>
        <option name="list.drilldown">none</option>
        <option name="refresh.display">progressbar</option>
      </event>
    </panel>
  </row>
  <row>
    <panel>
      <event>
        <title>Content Pattern Entity</title>
        <search>
          <query>index=logiq sourcetype=dglog host=$hosttok$ (EventType="CBaseExtractor::retrieveItems" "Match") OR (EventType="CBaseExtractor::retrieveItems" "matchesConsidered ==") $searchtok$</query>
          <earliest>$timetok.earliest$</earliest>
          <latest>$timetok.latest$</latest>
        </search>
        <option name="list.drilldown">none</option>
        <option name="refresh.display">progressbar</option>
      </event>
    </panel>
  </row>
</form>